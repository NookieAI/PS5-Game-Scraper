{
  "name": "ps5-game-scraper",
  "version": "1.7.0",
  "description": "PS5 Game Scraper",
  "author": "Nookie",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "build": {
    "appId": "com.nookie.ps5scraper",
    "productName": "PS5 Game Scraper",
    "icon": "assets/icon1.ico",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist/**/*",
      "!build/**/*",
      "!*.md",
      "!package-lock.json"
    ],
    "win": {
      "target": "portable",
      "icon": "assets/icon1.ico"
    }
  },
  "devDependencies": {
    "electron": "^25.9.8",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "1.6.7",
    "cheerio": "1.0.0-rc.12",
    "electron-store": "^8.2.0",
    "fuse.js": "^7.0.0"
  }
}